           _E_x_a_m_p_l_e_:

               {
                 type = "shellcommand";
                 command = "abook --mutt-query";
                 regexp = "'^(?P<email>[^@]+@[^\t]+)\t+(?P<name>[^\t]+)'";
                 ignorecase = "True";
               }

