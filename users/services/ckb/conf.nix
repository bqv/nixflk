    let
      cleanStr = lib.replaceStrings [ "{" "}" ] [ "%7B" "%7D" ];
      mkKeyValue =
        lib.generators.mkKeyValueDefault { inherit mkValueString; } "="
        (cleanStr k);
    in lib.concatStringsSep "\n" (map (kv: mkKeyValue kv) (lib.toList v));
